                              label.checkbox
                                input(type='checkbox', value='', data-toggle='checkbox')
                            td Unfollow 5 enemies from twitter
                            td.td-actions.text-right
                              button.btn.btn-info.btn-simple.btn-xs(type='button', rel='tooltip', title='Edit Task')
                                i.fa.fa-edit
                              |  
                              button.btn.btn-danger.btn-simple.btn-xs(type='button', rel='tooltip', title='Remove')
                                i.fa.fa-times
                    .footer
                      hr
                      .stats
                        i.fa.fa-history
                        |  Updated 3 minutes ago
        footer.footer
          .container-fluid
            nav.pull-left
              ul
                li
                  a(href='#') Home
                li
                  a(href='#') Company
                li
                  a(href='#') Portfolio
